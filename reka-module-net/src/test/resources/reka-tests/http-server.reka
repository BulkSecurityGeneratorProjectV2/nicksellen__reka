name http server to use for testing

net/http {
	listen localhost:8073
	on request {
		http/router {
			GET / {
				put response {
					content looks good
				}		
			}
			GET /with-params {
				putv response {
					content your name was :request.params.name
				}
			}
			GET /give-me-json {
				put response {
					content {
						thanks {
							for that
						}
					}
				}
			}
		}
	}
}